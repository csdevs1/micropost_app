<% provide(:title, 'All users') %>
<main class="mdl-layout__content mdl-color--grey-100 main-content">
    <h1>All users</h1>

    <%= will_paginate %>

    <ul lass="users-list-item mdl-list">
        <% @users.each do |user| %>
            <%= render user %>
        <% end %>
    </ul>

    <%= will_paginate %>
</main>